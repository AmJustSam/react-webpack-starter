{
  "presets": [
    [
      "@babel/preset-env",
       {
        "useBuiltIns": "entry",
        "corejs": "3.6.5",
         "targets": {
           "browsers": [
               "> 1%",
               "not ie 11",
               "not op_mini all"
           ]
         }
       }
    ],
    [
      "@babel/preset-react", { 
        "runtime": "automatic"
      }
    ]
  ],

  "plugins": [
     "@babel/plugin-proposal-class-properties"
  ]
}